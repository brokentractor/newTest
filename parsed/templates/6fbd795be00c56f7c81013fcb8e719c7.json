{"components/faceted-search/facets/hierarchy":"{{#if items}}\r\n<div class=\"accordion-block\">\r\n    <div\r\n        class=\"accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\r\n        role=\"button\"\r\n        data-collapsible=\"#facetedSearch-content--{{hyphenate facet }}\">\r\n        <h5 class=\"accordion-title\">\r\n            {{ title }}\r\n        </h5>\r\n\r\n        <div class=\"accordion-navigation-actions\">\r\n            {{#if selected}}\r\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\r\n            {{/if}}\r\n            <i class=\"fas fa-plus toggleLink-text--off\"></i>\r\n            <i class=\"fas fa-minus toggleLink-text--on\"></i>\r\n        </div>\r\n    </div>\r\n\r\n    <div id=\"facetedSearch-content--{{hyphenate facet }}\" class=\"accordion-content {{#unless ../start_collapsed }} is-open {{/unless}}\">\r\n        <ul id=\"facetedSearch-navList--{{hyphenate facet }}\" class=\"navList\" data-facet=\"{{facet}}\" data-has-more-results=\"{{ has_more_results }}\">\r\n            {{#each items}}\r\n                <li class=\"navList-item\">\r\n                    <a\r\n                        href=\"{{ url }}\"\r\n                        class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}} {{#if has_sub_categories }} has-children {{/if}}\"\r\n                        rel=\"nofollow\"\r\n                        data-id=\"{{ id }}\"\r\n                        data-faceted-search-facet>                                 \r\n                        <span class=\"checkicon\"><i class=\"fas fa-check-square\"></i></span>\r\n\r\n                        {{ title }}                        \r\n                    </a>\r\n\r\n\r\n                    {{#if children}}\r\n                        {{> components/faceted-search/facets/hierarchy-children children}}\r\n                    {{/if}}\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n\r\n        {{#if show_more_toggle}}\r\n            <a href=\"#facetedSearch-navList--{{hyphenate facet }}\" role=\"button\" class=\"toggleLink\">\r\n                <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.show-more'}}</span>\r\n            </a>\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n","components/faceted-search/facets/hierarchy-children":"<ul class=\"navList navList--inner\">\r\n    {{#each this}}\r\n        <li class=\"navList-item\">\r\n            <a\r\n            href=\"{{url}}\"\r\n            class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}} {{#if has_sub_categories }} has-children {{/if}}\"\r\n            rel=\"nofollow\"\r\n            data-id=\"{{ id }}\"\r\n            data-faceted-search-facet>\r\n                <span class=\"sqicon\"><i class=\"far fa-square\"></i></span>\r\n                <span class=\"checkicon\"><i class=\"fas fa-check-square\"></i></span>\r\n                {{title}}                \r\n            </a>\r\n            {{#if children}}\r\n                {{> components/faceted-search/facets/hierarchy-children children}}\r\n            {{/if}}\r\n        </li>\r\n    {{/each}}\r\n</ul>\r\n"}
