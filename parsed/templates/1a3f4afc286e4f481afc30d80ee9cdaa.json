{"components/products/list-itemtractor":"<tr class=\"productrow\">\r\n    <td class=\"pvcol REFID\">\r\n        {{#each custom_fields}}\r\n            {{#if name '===' 'REFID'}}\r\n                {{{value}}}\r\n            {{/if}}\r\n        {{/each}}\r\n    </td>\r\n    \r\n    <td class=\"pvcol image\">  \r\n        <a href=\"{{url}}\">\r\n            <img class=\"listItem-image\" src=\"{{getImage image 'productgallery_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n            <span class=\"img-seedet\">See Details</span>\r\n        </a>\r\n    </td>    \r\n\r\n    {{#each custom_fields}}\r\n        {{#if name '!=' 'REFID'}}\r\n            <td class=\"pvcol {{{name}}}\">{{{value}}}</td>\r\n        {{/if}}\r\n    {{/each}}\r\n   \r\n   \r\n    {{#if summary}}\r\n        <p style=\"display:none;\">{{{summary}}}</p>\r\n   {{/if}}\r\n\r\n</tr>\r\n\r\n"}
