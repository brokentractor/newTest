{"components/common/header":"{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    <div class=\"banner\">\r\n        {{{limit banners.top 1}}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n<div id=\"htop\">\r\n\t<div class=\"hinner\">\r\n    \t<span id=\"hphone\">CALL <a href\"tel:8009097060\">(800) 909-7060</a></span>\r\n        <div id=\"hlinks\">\r\n        \t<a href=\"/pages/parts-request/\">Parts Request</a>\r\n        \t<a href=\"/blog/\">Blog</a>\r\n        \t<a href=\"/pages/contact-us/\">Contact Us</a>\r\n        \t<span id=\"account-dd\">Account <i class=\"fa fa-angle-down\"></i></span>\r\n            <div id=\"accountMenu\">\r\n                <ul>\r\n                    {{#if customer}}\r\n                        <li><a href=\"/account.php\" id=\"accToplink\">My Account</a></li>\r\n                        <li><a href=\"/account.php?action=order_status\">Order Status</a></li>\r\n                        <li><a href=\"{{urls.auth.logout}}\">Sign Out</a></li>\r\n                     {{else}}\r\n                        <li><a href=\"/login.php\" id=\"accToplink\">SIGN IN</a></li>\r\n                        <li><a href=\"/account.php?action=order_status\">Order Status</a></li>\r\n                        <li><a href=\"/login.php?action=create_account\">Create Account</a></li>\r\n                    {{/if}}\r\n                </ul>\r\n            </div>\r\n        \t<a\r\n                data-cart-preview\r\n                data-dropdown=\"cart-preview-dropdown\"\r\n                data-options=\"align:right\"\r\n                href=\"{{urls.cart}}\"\r\n                id=\"cartlink\"\r\n                >\r\n                <i class=\"fa fa-shopping-cart\"></i>\r\n            </svg>\r\n            \t<strong>Cart <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">({{cart.quantity}})</span></strong>\r\n                <i class=\"fa fa-angle-down\"></i>\r\n            </a>\r\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n        </div>\r\n    </div>\r\n</div>\r\n<header class=\"header\" role=\"banner\">\r\n    <div class=\"header-inner\">\r\n        <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n            <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n        </a>\r\n\r\n        <a href=\"/\" id=\"logolink\">\r\n            <svg id=\"header-logo\"><use xlink:href=\"#icon-brokentractor-logo\" /></svg>\r\n        </a>\r\n\r\n        <a href=\"/cart.php\" id=\"mcartlink\">\r\n            <i class=\"fa fa-shopping-cart\"></i>\r\n\r\n            <span id=\"mcart-count\">{{cart.quantity}}</span>\r\n        </a>\r\n\r\n        <div id=\"header-search\">\r\n            {{> components/common/search-box}}\r\n        </div>\r\n\r\n        {{{region name=\"header_bottom\"}}}\r\n\r\n        <div id=\"shop-parts\">\r\n            Shop Parts\r\n            <svg class=\"svg-inline--fa fa-angle-down fa-w-10\" aria-hidden=\"true\" data-fa-processed=\"\" data-prefix=\"fa\" data-icon=\"angle-down\" role=\"img\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 320 512\"><path fill=\"currentColor\" d=\"M143 352.3L7 216.3c-9.4-9.4-9.4-24.6 0-33.9l22.6-22.6c9.4-9.4 24.6-9.4 33.9 0l96.4 96.4 96.4-96.4c9.4-9.4 24.6-9.4 33.9 0l22.6 22.6c9.4 9.4 9.4 24.6 0 33.9l-136 136c-9.2 9.4-24.4 9.4-33.8 0z\"></path></svg>\r\n        </div>\r\n\r\n\r\n        <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n            {{> components/common/navigation-menu}}\r\n        </div>\r\n    </div>\r\n</header>\r\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        <li class=\"navPage-subMenu-item-parent\">\r\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang '